%li.timeline-inverted
  .timeline-badge.success
    %i.glyphicon.glyphicon-floppy-saved
  .timeline-panel
    .timeline-heading
      %h4.timeline-title ADICIONADO #{ version.type_class }
      %p
        %small.text-muted
          %i.glyphicon.glyphicon-time
          #{ l version.created_at, format: :short } #{ ( user = version.try(:user).try(:full_name_or_email) ).present? ? "por #{ user }" : '' }
    .timeline-body
      %p 
        = version.changeset