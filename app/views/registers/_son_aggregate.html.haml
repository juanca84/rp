= f.simple_fields_for resources do |resource|
  /= resource.input :type_person
  = resource.simple_fields_for :person do |person|
    = person.input :name
    = person.input :birthdate, as: :string
    = person.input :sex, as: :select, collection: Person::SEX
    = resource.input :time_to_land, as: :string
    = person.input :identification
